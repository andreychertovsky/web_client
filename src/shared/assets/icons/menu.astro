---
interface Props {
  class?: string;
}

const { class: className } = Astro.props;
---

<svg
  class={className}
  xmlns="http://www.w3.org/2000/svg"
  width="24"
  height="24"
  fill="none"
  viewBox="0 0 24 24"
>
  <rect width="18" height="2" x="3" y="6" fill="currentColor" rx="1"></rect>
  <rect width="18" height="2" x="3" y="11" fill="currentColor" rx="1"></rect>
  <rect width="18" height="2" x="3" y="16" fill="currentColor" rx="1"></rect>
</svg>
